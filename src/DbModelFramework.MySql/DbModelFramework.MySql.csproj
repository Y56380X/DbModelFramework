<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.6.0</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Y56380X</Authors>
    <Company />
    <Copyright>Copyright (c) 2017-2020 Y56380X</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/Y56380X/DbModelFramework</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Pre-defined MySql and MariaDb DbRequirements for DbModelFramework</Description>
    <PackageVersion>1.6.0</PackageVersion>
