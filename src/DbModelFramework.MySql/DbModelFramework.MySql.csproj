  <ItemGroup>
    <None Include="../../LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\DbModelFramework\DbModelFramework.csproj" />
  </ItemGroup>

</Project>
